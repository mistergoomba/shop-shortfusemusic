    "@vendure/asset-server-plugin": "3.5.2",
    "@vendure/core": "3.5.2",
    "@vendure/email-plugin": "3.5.2",
    "@vendure/graphiql-plugin": "3.5.2",
    "dotenv": "16.5.0",
    "pg": "8.16.0",
    "concurrently": "^9.1.2"
  },
  "devDependencies": {
    "@vendure/cli": "3.5.2",
    "typescript": "5.8.2"
  }
}
